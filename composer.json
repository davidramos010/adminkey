{
    "name": "yiisoft/yii2-app-basic",
    "description": "Admin Keys AK",
    "keywords": ["yii2", "framework", "basic", "project template"],
    "homepage": "http://www.yiiframework.com/",
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/yii2/issues?state=open",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/yii2"
    },
    "minimum-stability": "dev",
    "require": {
        "php": "^7.4",
        "yiisoft/yii2": "~2.0.45",
        "yiisoft/yii2-bootstrap": "2.0.10",
        "yiisoft/yii2-swiftmailer": "2.1.1",
        "yiisoft/yii2-queue": "2.3.0",
        "yiisoft/yii2-sphinx": "2.0.12",
        "rmrevin/yii2-fontawesome": "2.17",
        "kartik-v/yii2-widgets": "3.4.1",
        "kartik-v/yii2-helpers": "1.3.7",
        "kartik-v/yii2-dropdown-x": "1.2.0",
        "kartik-v/yii2-checkbox-x": "1.0.3",
        "kartik-v/yii2-dialog": "1.0.3",
        "kartik-v/yii2-detail-view": "1.8.3",
        "kartik-v/yii2-popover-x": "1.3.4",
        "kartik-v/yii2-editable": "1.7.8",
        "kartik-v/yii2-money": "1.2.1",
        "kartik-v/yii2-nav-x": "1.2.4",
        "kartik-v/yii2-sortable": "1.2.1",
        "kartik-v/yii2-context-menu": "1.2.2",
        "kartik-v/yii2-icons": "1.4.4",
        "kartik-v/yii2-grid": "3.3.4",
        "kartik-v/yii2-dynagrid": "1.5.1",
        "kartik-v/yii2-export": "1.4.0",
        "kartik-v/yii2-mpdf": "1.0.6",
        "kartik-v/yii2-builder": "1.6.4",
        "kartik-v/yii2-datecontrol": "1.9.7",
        "hail812/yii2-adminlte3": "~1.1",
        "factorenergia/jpgraph": "dev-master",
        "yii2mod/yii2-sweet-alert": "1.3",
        "guzzlehttp/guzzle": "6.5.2",
        "frostealth/yii2-aws-s3": "1.0.0",
        "phpseclib/phpseclib": "2.0.11",
        "bethrezen/yii2-cached-rbac-dbmanager": "dev-master",
        "factorenergia/yii2-activerecord-history":"1.2.0",
        "raoul2000/yii2-workflow": "1.2.0",
        "krisengine/yii2-graylog2": "dev-master",
        "philippfrenzel/yii2fullcalendar": "3.9.0",
        "clegginabox/pdf-merger": "dev-master",
        "phpoffice/phpspreadsheet": "^1.11",
        "zircote/swagger-php": "^3.0",
        "bower-asset/swagger-ui": "^3.25",
        "2amigos/yii2-chartjs-widget": "3.0.1",
        "econea/nusoap": "^0.9.8",
        "kartik-v/yii2-date-range": "1.6.9",
        "dompdf/dompdf": "^0.8.5",
        "2amigos/yii2-tinymce-widget" : "~1.1",
        "kartik-v/yii2-number": "1.0.3",
        "kartik-v/yii2-tabs-x": "1.2.7",
        "vilochane/yii2-barcode-generator": "dev-master",
        "kartik-v/yii2-dropdown-x": "dev-master",
        "kartik-v/yii2-bootstrap4-dropdown": "@dev"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "~2.1.0",
        "yiisoft/yii2-gii": "~2.2.0",
        "yiisoft/yii2-faker": "~2.0.0",

        "codeception/base": "^2.2.3",
        "codeception/specify": "~0.4.3"
    },
    "autoload": {
        "psr-4": {
            "miloschuman\\highcharts\\": "src/",
            "kartik\\widgets\\": "",
            "kartik\\helpers\\": "",
            "kartik\\dropdown\\": "",
            "kartik\\checkbox\\": "",
            "kartik\\dialog\\": "",
            "kartik\\detail\\": "",
            "kartik\\popover\\": "",
            "kartik\\editable\\": "",
            "kartik\\money\\": "",
            "kartik\\nav\\": "",
            "kartik\\sortable\\": "",
            "kartik\\cmenu\\": "",
            "kartik\\icons\\": "",
            "kartik\\grid\\": "",
            "kartik\\dynagrid\\": "",
            "kartik\\export\\": ""
        }
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer" : true
        },
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        }
    },
    "scripts": {
        "post-install-cmd": [
            "yii\\composer\\Installer::postInstall"
        ],
        "post-create-project-cmd": [
            "yii\\composer\\Installer::postCreateProject",
            "yii\\composer\\Installer::postInstall"
        ]
    },
    "extra": {
        "yii\\composer\\Installer::postCreateProject": {
            "setPermission": [
                {
                    "runtime": "0777",
                    "web/assets": "0777",
                    "yii": "0755"
                }
            ]
        },
        "yii\\composer\\Installer::postInstall": {
            "generateCookieValidationKey": [
                "config/web.php"
            ]
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
